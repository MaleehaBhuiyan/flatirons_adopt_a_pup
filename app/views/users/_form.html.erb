<%= form_for(@user) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :profile_pic %>
    <%= f.text_field :user_img_url%>


    <%= f.submit %>

<% end %>

